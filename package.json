{
    "name": "@abualif1413/iraipl",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "prod": "NODE_ENV=production npm run prod-prep",
        "prod-prep": "npm run build-web-app && dotenvx run --env-file=.env -- pm2 start ./bin/www",
        "dev": "nodemon --ext js,jsx,css,scss,json,twig --ignore \"public/javascripts/\" --exec \"npx eslint web-app && npm run build-web-app && dotenvx run --env-file=.env.development -- node ./bin/www\"",
        "build-web-app": "esbuild web-app/index.js --bundle --minify --outfile=public/javascripts/web-app.js --loader:.js=jsx --loader:.woff=file --loader:.woff2=file",
        "db:migrate:create": "npx sequelize-cli migration:generate --name ",
        "db:migrate:up": "npx sequelize-cli db:migrate",
        "db-prod:migrate:up": "NODE_ENV=production npx sequelize-cli db:migrate",
        "db:migrate:down": "npx sequelize-cli db:migrate:undo:all",
        "db:seed:up": "npx sequelize-cli db:seed:all",
        "db-prod:seed:up": "NODE_ENV=production npx sequelize-cli db:seed:all",
        "db:seed:down": "npx sequelize-cli db:seed:undo:all",
        "formatting": "npx prettier . --write",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build"
    },
    "dependencies": {
        "@dotenvx/dotenvx": "^1.11.5",
        "@emotion/react": "^11.13.3",
        "@emotion/styled": "^11.13.0",
        "@fontsource/roboto": "^5.1.1",
        "@mui/icons-material": "^5.16.7",
        "@mui/material": "^5.16.7",
        "@ngrok/ngrok": "^1.4.1",
        "@tanstack/react-table": "^8.9.3",
        "axios": "^1.7.5",
        "compression": "^1.8.0",
        "cookie-parser": "~1.4.4",
        "debug": "~2.6.9",
        "esbuild": "^0.23.1",
        "express": "~4.16.1",
        "formik": "^2.4.6",
        "http-errors": "~1.6.3",
        "jsonwebtoken": "^9.0.2",
        "lodash": "^4.17.21",
        "moment": "^2.30.1",
        "morgan": "~1.9.1",
        "mysql2": "^3.11.2",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-number-format": "^5.4.4",
        "react-router-dom": "^6.26.1",
        "sequelize": "^6.37.3",
        "twig": "~0.10.3",
        "uri-js": "^4.4.1",
        "uuid": "^10.0.0",
        "yup": "^1.4.0"
    },
    "devDependencies": {
        "@chromatic-com/storybook": "^1.9.0",
        "@eslint/js": "^9.9.0",
        "@storybook/addon-essentials": "^8.3.6",
        "@storybook/addon-interactions": "^8.3.6",
        "@storybook/addon-links": "^8.3.6",
        "@storybook/addon-onboarding": "^8.3.6",
        "@storybook/addon-webpack5-compiler-swc": "^1.0.5",
        "@storybook/blocks": "^8.3.6",
        "@storybook/react": "^8.3.6",
        "@storybook/react-webpack5": "^8.3.6",
        "@storybook/test": "^8.3.6",
        "eslint": "^9.9.0",
        "eslint-plugin-react": "^7.35.0",
        "eslint-plugin-storybook": "^0.9.0",
        "globals": "^15.9.0",
        "nodemon": "^3.1.4",
        "prettier": "3.3.3",
        "prop-types": "^15.8.1",
        "sequelize-cli": "^6.6.2",
        "storybook": "^8.3.6"
    },
    "eslintConfig": {
        "extends": [
            "plugin:storybook/recommended"
        ]
    }
}
